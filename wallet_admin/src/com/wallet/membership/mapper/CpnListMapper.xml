    select
    <if test="distinct" >
      distinct
    </if>
    	<if test="rowsPerPage != null">
			top ${rowsPerPage}
		</if>
    	A.cpn_id
      ,ISNULL(A.RECOMM_YN,'N') AS RECOMM_YN
      ,(CASE WHEN ISNULL(A.RECOMM_SEQ,0)=0 THEN 4 ELSE A.RECOMM_SEQ END) AS RECOMM_SEQ
      ,A.main_display
      ,convert(integer,A.disp_order)AS disp_order
      ,A.name
      ,A.sday
      ,A.eday
      ,(SELECT memb_id FROM memb_cardlist E WHERE A.memb_id = E.MEMB_ID) AS memb_id
      ,(SELECT name FROM memb_cardlist E WHERE A.memb_id = E.MEMB_ID) AS MEMB_NAME
      ,(SELECT COMP_NAME FROM MW_CM_COMPANY E WHERE B.COMP_ID = E.COMP_ID) AS COMP_NAME
      ,A.part_v
    	,(CASE WHEN A.part_v='M' THEN '멤버십 쿠폰'
	        WHEN A.part_v='P' THEN '프릭스 쿠폰'
	        WHEN A.part_v='R' THEN '해외 쿠폰'
	        WHEN A.part_v='U' THEN '유비페이 쿠폰'
	        ELSE '일반/이벤트 쿠폰' END)AS part
	        
    FROM cpn_list A 
    INNER JOIN MW_CS_CPN_LIST B ON A.cpn_id = B.CPN_ID
    <if test="_parameter != null" >
      <include refid="Example_Where_Clause" />
    </if>
    <if test="orderByClause != null" >
      order by ${orderByClause}
    </if>
