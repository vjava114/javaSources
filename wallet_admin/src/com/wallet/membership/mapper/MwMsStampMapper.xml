    select
    <if test="distinct">
      distinct
    </if>
    <if test="rowsPerPage != null">
			top ${rowsPerPage}
	</if>
		 A.MEMB_ID
		,A.STAMP_UNIT
		,A.STAMP_IMAGE
		,A.STAMP_MAX_NO
		,A.EXPIRE_SDAY
		,A.EXPIRE_EDAY
		,A.REG_DTM
		,A.REG_USER
		,A.CHG_DTM
		,A.CHG_USER
		,A.BENEFIT_NOTICE
		,A.STAMP_SEQ
		,B.name
		,C.COMP_ID
		,C.COMP_NAME
			,(SELECT D.IMAGE_URL FROM MW_CM_IMAGE D WHERE D.ID = A.MEMB_ID AND D.OS_TYPE='01' AND D.USE_TYPE='05')AS STAMP_LIST_3GS 
			,(SELECT E.IMAGE_URL FROM MW_CM_IMAGE E WHERE E.ID = A.MEMB_ID AND E.OS_TYPE='02' AND E.USE_TYPE='05')AS STAMP_LIST_4S
			,(SELECT F.IMAGE_URL FROM MW_CM_IMAGE F WHERE F.ID = A.MEMB_ID AND F.OS_TYPE='11' AND F.USE_TYPE='05')AS STAMP_LIST_ANDROID

			,(CASE WHEN (SELECT COUNT(*) FROM MW_CM_IMAGE G WHERE G.USE_TYPE = '05' AND G.ID = A.MEMB_ID)=0 
		        THEN 'N' 
    		    ELSE 'Y'
			    END) AS IMG_YN

 		,(CONVERT(CHAR(10), A.REG_DTM, 23)) AS REG_DTM_STR
  		,(CONVERT(CHAR(10), A.CHG_DTM, 23)) AS CHG_DTM_STR  	
	    
		FROM MW_MS_STAMP A
		INNER JOIN memb_cardlist B ON B.memb_id = A.MEMB_ID
		INNER JOIN MW_CM_COMPANY C ON C.COMP_ID = B.COMP_ID
		

    <if test="_parameter != null">
      <include refid="Example_Where_Stamp_Clause" />
    </if>
